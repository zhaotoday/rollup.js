{
  "name": "apples",
  "moduleName": "SomeModule",
  "version": "0.0.3",
  "description": "use rollup to develop a npm package",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslintfix": "eslint src/**/*.js --fix",
    "eslint": "eslint src/**/*.js",
    "stylelint": "stylelint src/**/*.css",
    "dev": "better-npm-run dev",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "build": {
      "command": "node build",
      "env": {
        "BUILD": "production"
      }
    },
    "dev": {
      "command": "node build",
      "env": {
        "BUILD": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaotoday/rollup.git"
  },
  "keywords": [
    "npm",
    "rollup"
  ],
  "author": "zhaojintian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhaotoday/rollup/issues"
  },
  "homepage": "https://github.com/zhaotoday/rollup#readme",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "better-npm-run": "^0.1.0",
    "cli-color": "^1.2.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.8.0",
